<!DOCTYPE html>
<html lang="en">
<%- include('partials/head') %>
  </head>
  <body class="bg-gray-100 font-sans">
    <%- include('partials/header') %>

    <main
      class="container mx-auto bg-white shadow-lg rounded-lg p-8 mt-10 max-w-xl"
    >
      <h2 class="text-2xl text-red-500 font-bold text-center mb-6">
        Here are our registered home:
      </h2>
      <ul class="space-y-4">
  <% registeredHomes.forEach(home => { %>
    <li class=" bg-gray-100 p-4 rounded-lg shadow-md hover:shadow-lg transition duration-300">
      <p class=" text-lg text-gray-800"><%= home.houseName %></p>
    </li>
  <% }) %>
</ul>

    </main>
  </body>
</html>
